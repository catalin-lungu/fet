                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphRightIndent" Canvas.ZIndex="10" ToolTipText="Right Indent" Style="{StaticResource RightIndentThumbStyle}" RulerThumbType="RightIndent"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_TabStopMarker" Canvas.ZIndex="0" Foreground="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_FirstLineIndentMarker" RulerThumbType="FirstLineIndent" Canvas.ZIndex="0" Style="{StaticResource FirstLineIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_IndentMarker" RulerThumbType="Indent" Canvas.ZIndex="0" Style="{StaticResource HangingIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_LeftIndentMarker" RulerThumbType="LeftIndent" Canvas.ZIndex="0" Style="{StaticResource LeftIndentTransparentFillStyle}"/>
                                </Canvas>
                                <Canvas x:Name="PART_TabStopMarkersContainer" Grid.Row="1" Height="0"/>
                            </Grid>
                        </Canvas>
                        <Canvas x:Name="PART_VerticalCanvas"
                                Grid.Row="1"
                                Background="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                            <Grid Grid.Row="1" x:Name="PART_VerticalRulerContainer" Margin="5 0" Width="36">
                                <Canvas>
                                    <Rectangle x:Name="PART_TopMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="30"/>
                                    <Rectangle x:Name="PART_PageVerticalRectangle" Fill="{telerik:Windows8TouchResource ResourceKey=MainBrush}" Canvas.Left="0" Canvas.Top="0" Width="30"/>
                                    <Rectangle x:Name="PART_BottomMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="30"/>
                                </Canvas>
                                <Canvas x:Name="PART_VerticalMarkersContainer"/>
                                <Canvas x:Name="PART_VerticalThumbBackgroundsContainer"/>
                                <Canvas x:Name="PART_VerticalThumbsContainer"/>
                            </Grid>
                        </Canvas>
                        <Border x:Name="PART_TabStops"
                                Background="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                BorderBrush="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"
                                BorderThickness="1"
                                Width="30"
                                Height="30">
                            <telerikControlsRTBUI:RulerThumb x:Name="PART_RulerThumbTypeSelector" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" IsHitTestVisible="False"/>
                        </Border>
                        <Line x:Name="PART_MarkLine"
                                Grid.Row="1"
                                Grid.Column="1"
                                Stroke="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"
                                StrokeThickness="1"
                                StrokeDashArray="1 1 1 1 1 3"
                                Margin="3 2 0 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutModeSelectorStyle" TargetType="telerikControlsRTBUIStatusBar:LayoutModeSelector">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:LayoutModeSelector">
                    <StackPanel x:Name="LayoutRoot" Orientation="Horizontal" UseLayoutRounding="True">
                        <telerik:RadToggleButton x:Name="btnPrintLayout" IsTabStop="False" IsBackgroundVisible="False" VerticalAlignment="Center" Width="36" Height="36" Focusable="False">
                            <Image Stretch="None" Source="/Telerik.Windows.Controls.RichTextBoxUI;component/Images/StatusBar/print_layout.png"/>
                        </telerik:RadToggleButton>
                        <telerik:RadToggleButton x:Name="btnWebLayout" Margin="4 0 0 0" IsTabStop="False" IsBackgroundVisible="False" VerticalAlignment="Center" Width="36" Height="36" Focusable="False">
                            <Image Stretch="None" Source="/Telerik.Windows.Controls.RichTextBoxUI;component/Images/StatusBar/web_layout.png"/>
                        </telerik:RadToggleButton>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NonLinearSliderStyle" TargetType="telerikControlsRTBUIStatusBar:NonLinearSlider">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:NonLinearSlider">
                    <Grid x:Name="LayoutRoot">
                        <telerik:RadSlider x:Name="slider" Minimum="0" Maximum="1" LargeChange="0.05" SmallChange="0.005"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PercentComboBoxStyle" TargetType="telerikControlsRTBUIStatusBar:PercentComboBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:PercentComboBox">
                    <Grid x:Name="LayoutRoot">
                        <telerik:RadComboBox x:Name="comboPercent" IsEditable="True" SelectedIndex="3" IsTextSearchEnabled="False" VerticalAlignment="Center" Margin="0 5">
                            <telerik:RadComboBoxItem Content="25%"/>
                            <telerik:RadComboBoxItem Content="50%"/>
                            <telerik:RadComboBoxItem Content="75%"/>
                            <telerik:RadComboBoxItem Content="100%"/>
                            <telerik:RadComboBoxItem Content="150%"/>
                            <telerik:RadComboBoxItem Content="200%"/>
                            <telerik:RadComboBoxItem Content="500%"/>
                        </telerik:RadComboBox>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ZoomControllerStyle" TargetType="telerikControlsRTBUIStatusBar:ZoomController">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:ZoomController">
                    <Grid x:Name="LayoutRoot">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" MinWidth="90"/>
                            <ColumnDefinition Width="4"/>
                            <ColumnDefinition Width="*" MinWidth="150"/>
                            <ColumnDefinition Width="4"/>
                        </Grid.ColumnDefinitions>
                        <telerikControlsRTBUIStatusBar:PercentComboBox x:Name="comboZoom"/>
                        <telerikControlsRTBUIStatusBar:NonLinearSlider x:Name="sliderZoom" VerticalAlignment="Center" Grid.Column="2"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRichTextBoxStatusBarStyle" TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar">
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="BorderThickness" Value="0 1 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
                        <Grid x:Name="LayoutRoot" VerticalAlignment="Center" HorizontalAlignment="Right">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <telerikControlsRTBUIStatusBar:LayoutModeSelector x:Name="layoutModeSelector" Margin="0 0 6 0" VerticalAlignment="Center"/>
                            <telerikControlsRTBUIStatusBar:ZoomController x:Name="zoomController" Grid.Column="1" Margin="0 0 6 0"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ActionButtonStyle" TargetType="telerik:RadRibbonToggleButton">
        <Setter Property="Padding" Value="3"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="MinHeight" Value="36"/>
        <Setter Property="Foreground" Value="{telerik:Windows8TouchResource ResourceKey=MainForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadRibbonToggleButton">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver"/>
                                <VisualState x:Name="Pressed"/>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="contentPresenter" Storyboard.TargetProperty="Opacity">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0.5"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="CheckStates">
                                <VisualState x:Name="Unchecked"/>
                                <VisualState x:Name="Checked">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CheckedBorder" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused"/>
                                <VisualState x:Name="Unfocused"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="TextAlignment">
                                <VisualState x:Name="TextAtBottom"/>
                                <VisualState x:Name="TextAtRight"/>
                                <VisualState x:Name="TextHide"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border
                                CornerRadius="{StaticResource ControlOuterBorder_CornerRadius}"
                                Margin="1 0"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"/>
                        <Border x:Name="CheckedBorder"
                                Background="{telerik:Windows8TouchResource ResourceKey=LowBrush}"
                                BorderBrush="{telerik:Windows8TouchResource ResourceKey=LowBrush}"
                                BorderThickness="1"
                                MinHeight="{TemplateBinding MinHeight}"
                                CornerRadius="{StaticResource ControlOuterBorder_CornerRadius}"
                                Visibility="Collapsed"/>
                        <ContentPresenter x:Name="contentPresenter"
                                Margin="{TemplateBinding Padding}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MultipleUndoControlStyle" TargetType="telerikControlsRTBUI:MultipleUndoControl">
        <Setter Property="MinWidth" Value="120"/>
        <Setter Property="ActionButtonStyle" Value="{StaticResource ActionButtonStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:MultipleUndoControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel x:Name="actionButtonsPanel"/>
                        <telerik:RadButton x:Name="textBlockMultipleUndoActionsContent"
                                Grid.Row="1"
                                Margin="{StaticResource MultipleUndoControl_ButtonMargin}"
                                ClickMode="Press"
                                BorderThickness="0 1 0 0"
                                HorizontalContentAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbStyle}"/>
    <Style TargetType="telerikControlsRTBUI:DocumentRulerBase" BasedOn="{StaticResource DocumentRulerBaseStyle}"/>
    <Style TargetType="telerikControlsRTBUI:DocumentRuler" BasedOn="{StaticResource DocumentRulerBaseStyle}"/>
    <Style TargetType="telerikControlsRTBUIStatusBar:LayoutModeSelector" BasedOn="{StaticResource LayoutModeSelectorStyle}"/>
    <Style TargetType="telerikControlsRTBUIStatusBar:NonLinearSlider" BasedOn="{StaticResource NonLinearSliderStyle}"/>
    <Style TargetType="telerikControlsRTBUIStatusBar:PercentComboBox" BasedOn="{StaticResource PercentComboBoxStyle}"/>
    <Style TargetType="telerikControlsRTBUIStatusBar:ZoomController" BasedOn="{StaticResource ZoomControllerStyle}"/>
    <Style TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar" BasedOn="{StaticResource RadRichTextBoxStatusBarStyle}"/>
    <Style TargetType="telerikControlsRTBUI:MultipleUndoControl" BasedOn="{StaticResource MultipleUndoControlStyle}"/>
    <Style TargetType="telerikControlsRTBUI:RadRichTextBoxRibbonUI" BasedOn="{StaticResource RadRibbonViewStyle}"/>
</ResourceDictionary>
