                                                Fill="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                                Width="24"
                                                Height="24"/>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="PART_CloseButton"
                                            Command="{x:Static telerik:WindowCommands.Close}"
