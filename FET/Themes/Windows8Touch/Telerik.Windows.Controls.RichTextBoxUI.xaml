                                            <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                                        </ToolTipService.ToolTip>
                                        <Grid>
                                            <Ellipse Stretch="Fill" Stroke="{telerik:Windows8TouchResource ResourceKey=MainBrush}" StrokeThickness="2" Width="30" Height="30"/>
